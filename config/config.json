{
    "app": {
        "port": 3000
    },
    "db": {
        "host": "C:/GitHub/Basic-LDES-Server-Examples/database.sqlite",
        "maxCount":10000000
    },
    "sources": [
        {
            "route": "/feed/langemunt/observations",
            "sourceFile": "dist/sources/observationSource",
            "environment": "gent_langemunt",
            "topic": "/gent/gent_langemunt",
            "username": "opendata",
            "observationsPerPage":60
        },
        {
            "route": "/feed/veldstraat/observations",
            "sourceFile": "dist/sources/observationSource",
            "environment": "veldstraat",
            "topic": "/gent/veldstraat",
            "username": "opendata",
            "observationsPerPage": 200
        },
        {
            "route": "/feed/public/zones/v1",
            "environment":"zones",
            "sourceFile": "dist/sources/zoneSource"
        },
        {
            "route":"/catalog",
            "environment":"catalog",
            "sourceFile":"dist/sources/catalogSource"
        }
    ]
}